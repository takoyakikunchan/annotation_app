<%= render "shared/header" %>
<div class='main'>
 <div class='song-contents'>
  <ul class='song-lists'>
 <% @songs.each do |song| %>
 <li class=' song-list' >
 <%= link_to song_path(song.id) do %>
  <div class='song-info'>
   <h3 class='song-name'>
     <%= song.name %>
   </h3>
   <h3 class='song-image'>
     <%= image_tag song.image, class: "item-img" %>
   </h3>
  <%song.artists.each do |a| %>
      <%=a.art_name%>
        <%end%>

  </div>
 <% end %>
 </li>
 <% end %>

 </ul>
 </div>
</div>
<%= render "shared/footer" %>